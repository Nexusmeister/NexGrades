<Page x:Class="NexGrades.App.Features.Home.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:home="clr-namespace:NexGrades.App.Features.Home"
      d:DataContext="{d:DesignInstance Type=home:HomeViewModel}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage">

    <StackPanel>
        <ui:Card Content="{Binding WelcomeMessage}"></ui:Card>
    </StackPanel>
</Page>
